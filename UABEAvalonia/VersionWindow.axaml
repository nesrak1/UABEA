<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450"
        Width="300" Height="450"
        x:Class="UABEAvalonia.VersionWindow"
        Title="Select class version">
  <Grid>
    <TextBlock Margin="10,10,10,25" Name="infoLbl">
There is no type database for [version].
Please choose the closest version.
    </TextBlock>
    <ListBox Margin="10,45,10,45" Name="boxVersionList">
      <ListBox.Items>
        <ListBoxItem>Test</ListBoxItem>
        <ListBoxItem>Stuff</ListBoxItem>
        <ListBoxItem>Goes</ListBoxItem>
        <ListBoxItem>Here</ListBoxItem>
      </ListBox.Items>
    </ListBox>
    <Grid Margin="10,10,10,10" VerticalAlignment="Bottom">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*"></ColumnDefinition>
        <ColumnDefinition Width="*"></ColumnDefinition>
      </Grid.ColumnDefinitions>
      <Button Grid.Column="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Name="btnOk">Ok</Button>
      <Button Grid.Column="1" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Name="btnCancel">Cancel</Button>
    </Grid>
  </Grid>
</Window>
